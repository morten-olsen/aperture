#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules/msw/cli/node_modules:/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules/msw/node_modules:/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules:/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules/msw/cli/node_modules:/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules/msw/node_modules:/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules:/Users/alice/Projects/private/incubator/glados/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules/msw/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/msw@2.12.10_@types+node@25.2.3_typescript@5.9.3/node_modules/msw/cli/index.js" "$@"
fi
